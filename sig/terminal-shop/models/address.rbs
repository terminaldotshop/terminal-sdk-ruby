module TerminalShop
  module Models
    type address_api =
      {
        id: String,
        city: String,
        country: String,
        name: String,
        :street1 => String,
        zip: String,
        phone: String,
        province: String,
        :street2 => String
      }

    class AddressAPI < TerminalShop::BaseModel
      attr_accessor id: String

      attr_accessor city: String

      attr_accessor country: String

      attr_accessor name: String

      attr_accessor street1: String

      attr_accessor zip: String

      attr_reader phone: String?

      def phone=: (String) -> String

      attr_reader province: String?

      def province=: (String) -> String

      attr_reader street2: String?

      def street2=: (String) -> String

      def initialize:
        (
          id: String,
          city: String,
          country: String,
          name: String,
          street1: String,
          zip: String,
          phone: String,
          province: String,
          street2: String
        ) -> void
        | (
          ?TerminalShop::Models::address_api | TerminalShop::BaseModel data
        ) -> void

      def to_hash: -> TerminalShop::Models::address_api
    end
  end
end
